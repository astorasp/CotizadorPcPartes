{
  "name": "confluent-pedidos-sink-connector",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "connection.url": "jdbc:mysql://mysql-pedidos:3306/cotizador_pedidos_db?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=America/Mazatlan",
    "connection.user": "debezium",
    "connection.password": "dbz_password",
    "topics": "componentes.cotizador_componentes_db.cocomponente",
    "table.name.format": "cocomponente",
    "insert.mode": "upsert",
    "delete.enabled": "true",
    "pk.mode": "record_key",
    "pk.fields": "id_componente",
    "auto.create": "false",
    "auto.evolve": "false",
    "batch.size": "100",
    "max.retries": "10",
    "retry.backoff.ms": "3000",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "drop"
  }
}